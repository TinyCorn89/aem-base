<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    boxMaxWidth="800"
    height="500"
    title="AdFinder Search"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="AdFinder Search Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <titleLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Title"
                        name="./titleLabel"
                        xtype="textfield"/>
                    <keywordsLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="KeywordsLabel"
                        name="./keywordsLabel"
                        xtype="textfield"/>
                    <dateLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Date Label"
                        name="./dateLabel"
                        xtype="textfield"/>
                    <buttonLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Button Label"
                        name="./buttonLabel"
                        xtype="textfield"/>
                    <advertiserLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="AdvertiserLabel"
                        name="./advertiserLabel"
                        xtype="textfield"/>
                    <adLabel
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Ad Label"
                        name="./adLabel"
                        xtype="textfield"/>
                    <searchPath
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Search Path"
                        name="./searchPath"
                        rootPath="/etc"
                        xtype="pathfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
