<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    boxMaxWidth="800"
    height="500"
    title="Latest Article"
    width="800"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Latest Article Configuration">
                <items jcr:primaryType="cq:WidgetCollection">
                    <parentpage
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="Articles Parent Page"
                        name="./parentPage"
                        rootPath="/content/launchpad"
                        xtype="pathfield"/>
                    <noOfItems
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Number Of Items"
                        minValue="{Long}1"
                        name="./numberOfItems"
                        xtype="numberfield"/>
                    <seealllinktext
                        jcr:primaryType="cq:Widget"
                        allowBlank="{Boolean}false"
                        fieldLabel="See All Link Text"
                        name="./seeAllLinkText"
                        xtype="textfield"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
