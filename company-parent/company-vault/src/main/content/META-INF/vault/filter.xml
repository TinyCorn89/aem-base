<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
	<filter root="/apps/company" />
	
	<filter root="/etc/clientcontext/default" />
    <filter root="/etc/designs/company">
        <include pattern="/etc/designs/company/clientlib_base"/>
        <include pattern="/etc/designs/company/clientlib_cq"/>
        <include pattern="/etc/designs/company/components"/>
    </filter>
	
    <filter root="/etc/designs/company/jcr:content" mode="merge">
		<include pattern="/etc/designs/company/jcr:content(/*)?"/>
	</filter>
	
	<filter root="/etc/designs/libraries" />
	<filter root="/etc/scaffolding/company" />
	<filter root="/etc/tags/company" />
	<filter root="/etc/domainAssets">
		<exclude pattern="/etc/domainAssets/scaffolding/.*" />
	</filter>
	<filter root="/apps/sling/" />
	<filter root="/services-sitemap" />
</workspaceFilter>